{
	"name": "Extensión PatmOS",
	"version": "2.2",
	"manifest_version": 3,
	"author": "Eduardo Antonio Arce Saavedra",
	"description": "Extensión para ver cursos y tareas pendientes del PatmOS",
	"permissions": ["storage"],
	"host_permissions": [
	  "https://patmos.upeu.edu.pe/*"
	],
	"action": {
	  "default_title": "Extensión PatmOS",
	  "default_popup": "popup.html",
	  "default_icon": {
		"16": "logo16.png",
		"48": "logo48.png",
		"128": "logo128.png"
	  }
	}
  }
